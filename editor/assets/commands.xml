            </combination>
        </command>
        <command name="Up" uniqueName="move_up_command" description="">
            <context>FOCUSED</context>
            <combination type="keyboard" repeat="true">
                UP
            </combination>
        </command>
        <command name="Jumpy up" uniqueName="move_jumpy_up_command" description="">
            <context>FOCUSED</context>
            <combination type="keyboard" repeat="true">
                SHIFT UP
            </combination>
        </command>
        <command name="Down" uniqueName="move_down_command" description="">
            <context>FOCUSED</context>
            <combination type="keyboard" repeat="true">
                DOWN
            </combination>
        </command>
        <command name="Jumpy Down" uniqueName="move_jumpy_down_command" description="">
