            <context>FOCUSED</context>>
            <combination type = "keyboard" repeat = "false">
                DEL
            </combination>
            <combination type = "keyboard" repeat = "false">
                FORWARD_DEL
            </combination>
        </command>
        <command name = "Rename" uniqueName = "rename_command" description = "">
            <context>FOCUSED</context>>
            <combination type = "keyboard" repeat = "false">
                F2
            </combination>
        </command>
        <command name = "Group" uniqueName = "group_command" description = "">
            <context>FOCUSED</context>>
