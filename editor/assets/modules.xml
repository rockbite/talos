<modules classPath = "com.talosvfx.talos.runtime.modules">
    <category name = "Main Modules">
        <module name = "Particle" wrapper = "ParticleModuleWrapper">ParticleModule</module>
        <module name = "Emitter" wrapper = "EmitterModuleWrapper">EmitterModule</module>
        <module name = "Render" wrapper = "DrawableModuleWrapper">DrawableModule</module>
    </category>
    <category name = "PointGenerators">
        <module name = "Single Point" wrapper = "SingleParticlePointDataGeneratorModuleWrapper">SingleParticlePointDataGeneratorModule</module>
        <module name = "FromTo Point" wrapper = "FromToParticlePointDataGeneratorModuleWrapper">FromToParticlePointDataGeneratorModule</module>
        <module name = "History Point" wrapper = "HistoryParticlePointDataGeneratorModuleWrapper">HistoryParticlePointDataGeneratorModule</module>
    </category>
    <category name = "MeshGenerators">
        <module name = "Quad" wrapper = "QuadMeshGeneratorModuleWrapper">QuadMeshGeneratorModule</module>
        <module name = "Strip" wrapper = "StripMeshGeneratorModuleWrapper">StripMeshGeneratorModule</module>
        <module name = "Marched" wrapper = "MarchedMeshGeneratorModuleWrapper">MarchedMeshGeneratorModule</module>
    </category>
    <category name = "General Use">
        <module name = "System Input" wrapper = "InputModuleWrapper">InputModule</module>
        <module name = "Global Values" wrapper = "GlobalScopeModuleWrapper">GlobalScopeModule</module>
        <module name = "Mixer" wrapper = "MixModuleWrapper">MixModule</module>
        <module name = "Shape Range" wrapper = "OffsetModuleWrapper">OffsetModule</module>
<!--        <module name = "Shape Coordinate" wrapper = "ShapeModuleWrapper">ShapeModule</module>-->
    </category>
    <category name = "Static Values">
        <module name = "Number" wrapper = "StaticValueModuleWrapper">StaticValueModule</module>
        <module name = "Position (Vector2)" wrapper = "Vector2ModuleWrapper">Vector2Module</module>
        <module name = "Position (Vector3)" wrapper = "Vector3ModuleWrapper">Vector3Module</module>
        <module name = "Color" wrapper = "ColorModuleWrapper">ColorModule</module>
        <module name = "Position Split (Vector)" wrapper = "VectorSplitModuleWrapper">VectorSplitModule</module>
    </category>
    <category name = "Color">
        <module name = "Color" wrapper = "ColorModuleWrapper">ColorModule</module>
        <module name = "Gradient" wrapper = "GradientColorModuleWrapper">GradientColorModule</module>
    </category>
    <category name = "Rendering">
        <module name = "Ribbon Renderer" wrapper = "RibbonModuleWrapper">RibbonModule</module>
        <module name = "Nine Patch Renderer" wrapper = "NinePatchModuleWrapper">NinePatchModule</module>
    </category>
    <category name = "Materials">
        <module name = "SpriteMaterial" wrapper = "SpriteMaterialModuleWrapper">SpriteMaterialModule</module>
    </category>
    <category name = "Randomness">
        <module name = "Random Range" wrapper = "RandomRangeModuleWrapper">RandomRangeModule</module>
        <module name = "Dynamic Random Range" wrapper = "DynamicRangeModuleWrapper">DynamicRangeModule</module>
        <module name = "Noise" wrapper = "NoiseModuleWrapper">NoiseModule</module>
        <module name = "Multi Input" wrapper = "RandomInputModuleWrapper">RandomInputModule</module>
    </category>
    <category name = "Motion and Physics">
        <module name = "Fake Motion Blur" wrapper = "FakeMotionBlurModuleWrapper">FakeMotionBlurModule</module>
        <module name = "Vector Field" wrapper = "VectorFieldModuleWrapper">VectorFieldModule</module>
        <module name = "Attractor" wrapper = "AttractorModuleWrapper">AttractorModule</module>
        <module name = "ForceApplier" wrapper = "ForceApplierModuleWrapper">ForceApplierModule</module>
    </category>
    <category name = "Curves and Math">
        <module name = "Curve" wrapper = "CurveModuleWrapper">CurveModule</module>
        <module name = "Interpolation" wrapper = "InterpolationWrapper">InterpolationModule</module>
        <module name = "Math Operations" wrapper = "MathModuleWrapper">MathModule</module>
        <module name = "Target Movement" wrapper = "TargetModuleWrapper">TargetModule</module>
    </category>
    <category name = "Adapters">
        <module name = "Beam Position" wrapper = "FromToModuleWrapper">FromToModule</module>
        <module name = "Cartesian to Radial" wrapper = "CartToRadModuleWrapper">CartToRadModule</module>
        <module name = "Radial to Cartesian" wrapper = "RadToCartModuleWrapper">RadToCartModule</module>
    </category>
    <category name = "Advanced">
        <module name = "Emitter Config" wrapper = "EmConfigModuleWrapper">EmConfigModule</module>
        <module name = "Scope" wrapper = "ScopeModuleWrapper">NANModule</module>
        <module name = "Script (Java)" wrapper = "ScriptModuleWrapper">ScriptModule</module>
    </category>
</modules>
